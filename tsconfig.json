{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "app/auth/login/page.jsx", "services/_config.js", "services/verification.service.js", "features/account/components/EducationalBackgroundSection.jsx", "features/account/components/UpdateEducationalBackground.jsx", "features/dashboard/admin/EmployerVerificationQueue.jsx", "features/reset-password/ResetPasswordPage.jsx", "features/dashboard/admin/GenderDistribution.jsx", "features/dashboard/admin/HighestEducationDistribution.jsx", "features/auth/VerifyEmailPage.jsx"  ],
  "exclude": ["node_modules"]
}
